{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "minAge": {
            "oneOf":[
                {
                    "type": "number",
                    "minimum": 18
                },
                {"type": "null"}
            ]
        },
        "maxAge": {
            "oneOf": [
                {
                    "type": "number",
                    "maximum": 65
                },
                {"type": "null"}
            ]
        },
        "gender": {
            "oneOf": [
                {"enum": ["male", "female"]},
                {"type": "null"}
            ]
        }
    }
}
